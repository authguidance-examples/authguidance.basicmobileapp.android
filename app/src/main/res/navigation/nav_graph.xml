<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/nav_graph"
            app:startDestination="@id/blankFragment">

    <fragment
        android:id="@+id/blankFragment"
        android:name="com.authguidance.basicmobileapp.logic.fragments.BlankFragment"
        android:label="Startup"
        tools:layout="@layout/fragment_blank">
    </fragment>
    <fragment
            android:id="@+id/companiesFragment"
            android:name="com.authguidance.basicmobileapp.logic.fragments.CompaniesFragment"
            android:label="Company List"
            tools:layout="@layout/fragment_companies">
    </fragment>
    <fragment
            android:id="@+id/transactionsFragment"
            android:name="com.authguidance.basicmobileapp.logic.fragments.TransactionsFragment"
            android:label="Company Transactions"
            tools:layout="@layout/fragment_transactions">
    </fragment>
    <fragment
            android:id="@+id/loginRequiredFragment"
            android:name="com.authguidance.basicmobileapp.logic.fragments.LoginRequiredFragment"
            android:label="Error Details"
            tools:layout="@layout/fragment_login_required">
    </fragment>
    <fragment
            android:id="@+id/errorFragment"
            android:name="com.authguidance.basicmobileapp.logic.fragments.ErrorFragment"
            android:label="Error Details"
            tools:layout="@layout/fragment_error">
    </fragment>
</navigation>